{
  "name": "angularBrowserifyBoilerplate",
  "version": "0.0.1",
  "config": {
    "dirs": {
      "sass": "./assets/css/scss",
      "icons": "./assets/icons",
      "imgSrc": "./assets/img",
      "css": "./assets/css",
      "img": "./assets/img",
      "fonts": "./assets/fonts",
      "js": "./assets/js",
      "jsSrc": "./app"
    },
    "cleanFiles": [
      "node_modules",
      ".sass-cache",
      "bower_components"
    ],
    "copyFiles": {
      "./app/libs/vendor/components-font-awesome/fonts/*": "./assets/fonts/"
    }
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "browserify-shim": "^3.8.2",
    "grunt": "^0.4.5",
    "grunt-angular-templates": "^0.5.7",
    "grunt-autoprefixer": "^1.0.1",
    "grunt-available-tasks": "^0.5.0",
    "grunt-browserify": "^3.3.0",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-compass": "^0.8.0",
    "grunt-contrib-requirejs": "^0.4.4",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-shell": "^1.1.1"
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "angular-growl": "./bower_components/angular-growl-v2/build/angular-growl.js",
    "angular-sweetalert": "./bower_components/angular-sweetalert/SweetAlert.js",
    "sweet-alert": "./bower_components/sweetalert/lib/sweet-alert.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": "angular",
    "angular-animate": {
      "depends": [
        "angular"
      ]
    },
    "angular-growl": {
      "depends": [
        "angular"
      ]
    },
    "angular-ui-router": {
      "depends": [
        "angular"
      ]
    },
    "angular-sanitize": {
      "depends": [
        "angular"
      ]
    },
    "angular-sweetalert": {
      "depends": [
        "angular",
        "sweet-alert:swal"
      ]
    }
  },
  "dependencies": {
    "angular": "^1.3.13",
    "angular-animate": "^1.3.13",
    "angular-loading-bar": "^0.6.0",
    "angular-route": "^1.3.13",
    "angular-sanitize": "^1.3.13",
    "jquery": "^2.1.3",
    "ng-dialog": "^0.3.11",
    "throbber": "^1.0.1"
  }
}